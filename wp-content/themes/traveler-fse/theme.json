{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "traveler-fse-theme",
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank"
		},
		{
			"name": "blog-alternative",
			"postTypes": [
				"page"
			],
			"title": "Blog (Alternative)"
		},
		{
			"name": "404",
			"postTypes": [
				"page"
			],
			"title": "404"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#1C1C1C",
					"name": "Shade 0",
					"slug": "shade-0"
				},
				{
					"color": "#707070",
					"name": "Shade 30",
					"slug": "shade-30"
				},
				{
					"color": "#979797",
					"name": "Shade 40",
					"slug": "shade-40"
				},
				{
					"color": "#CECECE",
					"name": "Shade 50",
					"slug": "shade-50"
				},
				{
					"color": "##f4f4f4",
					"name": "Shade 90",
					"slug": "shade-90"
				},
				{
					"color": "#ffffff",
					"name": "Shade 100",
					"slug": "shade-100"
				},
				{
					"color": "#FF9662",
					"name": "Accent 0",
					"slug": "accent-0"
				}
			]
		},
		"layout": {
			"contentSize": "1250px"
		},
		"spacing": {
			"blockGap": null,
			"customSpacingSize": true,
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "30",
					"name": "1"
				},
				{
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40",
					"name": "2"
				},
				{
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50",
					"name": "3"
				},
				{
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60",
					"name": "4"
				},
				{
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70",
					"name": "5"
				},
				{
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "80",
					"name": "6"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "Helvetica Neue",
					"slug": "primary"
				},
				{
					"fontFamily": "-TimesNewRomanPSMT, TimesNewRoman, 'Times New Roman', Times, Baskerville, Georgia, Cambria, Times New Roman, Times, serif",
					"name": "Times New Roman",
					"slug": "secondary"
				}
			],
			"fontSizes": [
				{
					"name": "Heading Section",
					"slug": "heading-section",
					"size": "20px"
				},
				{
					"name": "Heading Side Section",
					"slug": "heading-side-section",
					"size": "18px"
				},
				{
					"name": "Heading Page Title",
					"slug": "heading-page-title",
					"size": "48px"
				},
				{
					"name": "Heading Hero Title",
					"slug": "heading-hero-title",
					"size": "112px"
				},
				{
					"name": "Heading Large",
					"slug": "heading-large",
					"size": "34px"
				},
				{
					"name": "Heading Small",
					"slug": "heading-small",
					"size": "20px"
				},
				{
					"name": "Heading Italic",
					"slug": "heading-italic",
					"size": "20px"
				},
				{
					"name": "Heading Post",
					"slug": "heading-post",
					"size": "24px"
				},
				{
					"name": "Heading Post Title",
					"slug": "heading-post-title",
					"size": "48px"
				},
				{
					"name": "Paragraph excerpt",
					"slug": "paragraph-excerpt",
					"size": "20px"
				},
				{
					"name": "Paragraph Short Intro",
					"slug": "paragraph-short-intro",
					"size": "32px"
				},
				{
					"name": "Paragraph First Letter",
					"slug": "paragraph-first-letter",
					"size": "26px"
				},
				{
					"name": "Paragraph Post",
					"slug": "paragraph-post",
					"size": "19px"
				},
				{
					"name": "Hero Caption",
					"slug": "hero-caption",
					"size": "14px"
				},
				{
					"fluid": {
						"min": "0.875rem",
						"max": "1rem"
					},
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "1.875rem"
					},
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"fluid": false,
					"size": "2.25rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "4rem",
						"max": "10rem"
					},
					"size": "10rem",
					"slug": "xx-large"
				}
			]
		},
		"blocks": {
			"core/heading": {
				"spacing": {
					"padding": false
				}
			}
		},
		"custom": {
			"font-weight": {
				"light": "300",
				"normal": "400",
				"bold": "700"
			},
			"heading-hover-transition": "all 0.2s ease 0s",
			"heading-section": {
				"bottom-border-width": "0.3rem",
				"line-height": "30px",
				"letter-spacing": "-0.3px"
			},
			"heading-side-section": {
				"bottom-top-width": "0.3rem",
				"line-height": "18px",
				"letter-spacing": "-0.3px"
			},
			"heading-page-title": {
				"line-height": "67px",
				"letter-spacing": "0px"
			},
			"heading-hero-title": {
				"line-height": "112px",
				"letter-spacing": "0px"
			},
			"heading-large": {
				"line-height": "38.3px",
				"letter-spacing": "-0.4px"
			},
			"heading-small": {
				"line-height": "24px",
				"letter-spacing": "-0.2px"
			},
			"heading-post": {
				"line-height": "33.6px",
				"letter-spacing": "0px"
			},
			"heading-post-title": {
				"line-height": "54px",
				"letter-spacing": "-0.5px"
			},
			"paragraph-excerpt": {
				"line-height": "28px",
				"letter-spacing": "0px"
			},
			"paragraph-short-intro": {
				"line-height": "1.2",
				"letter-spacing": "0px"
			},
			"paragraph-first-letter": {
				"line-height": "34.1px",
				"letter-spacing": "0px"
			},
			"paragraph-post": {
				"line-height": "30.4px",
				"letter-spacing": "0px"
			}
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--shade-100)",
			"text": "var(--wp--preset--color--shade-0)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--font-primary)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		},
		"blocks": {
			"core/heading": {
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				}
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--shade-0)"
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		}
	}
}
